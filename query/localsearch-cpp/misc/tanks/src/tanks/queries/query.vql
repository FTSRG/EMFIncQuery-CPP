package tanks.queries

import "http://factory.ecore/tanks"
import "http://www.eclipse.org/emf/2002/Ecore"

@Bind(parameters={tankID, allapotID, homerseklet})
@QueryBasedFeature(feature = "allapot")
pattern szilard(tank, tankID : EInt, allapot : Szilard, allapotID : EInt, homerseklet : EDouble){
	Viz(viz);
	Tank.tarol(tank, viz);
	Tank.ID(tank, tankID);
	Szilard.ID(allapot, allapotID);
	check(homerseklet < 0);
}