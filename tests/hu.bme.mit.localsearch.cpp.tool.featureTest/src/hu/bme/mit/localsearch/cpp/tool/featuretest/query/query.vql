@Bind(parameters={robot})
pattern partsOfRobot(robot: Robot, robotpart: RobotPart){
	//Robot(robot);
	RobotPart(robotpart);
	Robot.parts(robot,robotpart);
}
@Bind(parameters={rob, N})
pattern robotPartCount(rob, N) {
	Robot(rob);
    N == count find partsOfRobot(rob, _);
}
